; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Servicio_Social"
config/version="0.0.1"
run/main_scene="res://scenes/splash_screen/SplashScreenManager.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://scripts/autoloads/sound/default_bus_layout.tres"

[autoload]

Persistence="*res://scripts/autoloads/data/Persistence.gd"
PlayerSession="*res://scripts/autoloads/data/PlayerSession.gd"
CharactersData="*res://scripts/autoloads/data/json_data/CharactersData.gd"
Mouse="*res://scripts/autoloads/mouse/Mouse.gd"
SceneLoader="*res://scripts/autoloads/scene_loader/SceneLoader.gd"
SceneTransition="*res://scripts/autoloads/scene_transition/SceneTransition.tscn"
ClosingGame="*res://scripts/autoloads/closing_game/ClosingGame.gd"
BackgroundMusic="*res://scripts/autoloads/sound/background_music/BackgroundMusic.tscn"
Sfx="*res://scripts/autoloads/sound/sfx/SFX.tscn"

[display]

window/size/viewport_width=1024
window/size/viewport_height=576
window/size/initial_position_type=0
window/size/always_on_top=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Servicio_Social"

[editor_plugins]

enabled=PackedStringArray("res://addons/ActionIcon/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/custom_font="res://assets/graphical_assets/user_interface/fonts/nokiafc22.ttf"
theme/default_font_antialiasing=2
theme/default_font_hinting=2
theme/default_font_multichannel_signed_distance_field=true

